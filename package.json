{
  "name": "cotter",
  "version": "0.3.10-beta.0",
  "description": "Cotter web SDK for JavaScript",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:umd": "cross-env FILE_NAME=cotter-js webpack",
    "build:umd:min": "cross-env NODE_ENV=production npm run build:umd",
    "clean": "rimraf lib dist",
    "build": "npm run clean && tsc && npm run build:umd && npm run build:umd:min"
  },
  "repository": {
    "type": "git",
    "url": "github.com/cotterapp/cotter-js"
  },
  "keywords": [
    "login",
    "authentication",
    "signup",
    "form",
    "register",
    "email",
    "verification",
    "phone"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cross-env": "^7.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "files": [
    "lib/**/*",
    "dist"
  ],
  "dependencies": {
    "axios": "^0.19.2",
    "cotter-token-js": "^0.1.5",
    "detect-node": "^2.0.4"
  }
}
